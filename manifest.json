{
    "manifest_version": 3,
    "name": "HanaGPT",
    "version": "2.0",
    "description": "Dibuat oleh Yuhans dan bertujuan untuk menyimpan sesi Perplexity secara aman.",
    "permissions": ["cookies", "storage"],
    "host_permissions": ["https://*.perplexity.ai/*"],
    "icons": {
      "16": "icons/perplexity1.png",
      "48": "icons/perplexity1.png",
      "128": "icons/perplexity1.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  